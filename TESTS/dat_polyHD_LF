
here=`pwd`
DIR_EVRT=$here/..

$DIR_EVRT/Tnum90.exe << ** > res
 &variables Old_Qtransfo=f nb_Qtransfo=3 Cart_Type='LF' cos_th=f /
 &Coord_transfo name_transfo='bunch_poly' nb_vect=1 inTOout=f cos_beta=t /
1. 2.  (H H)

 &Coord_transfo name_transfo='poly' /
     &vector nb_vect=0 Frame=t iAtA=1 iAtB=2 /

 &Coord_transfo name_transfo='active' /
1   1 1     1 1 1

 &minimum  /
RHH 1. angs
alpha  0.2
ubeta  0.3
xCOM   1.
yCOM   2.
zCOM   3.

 &calculation calc_Tnum=f calc_gG=t test=t nderivGg=0 /
**

TnumOK=`tail -1 res | grep -c "END Tnum" `
if [ $TnumOK -ne "1" ]
then
 echo $0  1
 exit
fi

G11=`awk '/d0G/ {print $2}' res`

mH=1.
mD=2.

err1=`echo $G11 $mH $mD | LANG=c awk '{print $1-(1/$2+1/$3)}'`
err2=0.0

echo $0 $err1 $err2